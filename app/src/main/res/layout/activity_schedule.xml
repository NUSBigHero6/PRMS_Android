<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:orientation="vertical"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:padding="16dp"
tools:context=".schedule.android.ui.ScheduleScreen">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Date"
        tools:text="DateofProgram" />
    <EditText
        android:id="@+id/maintain_Date_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Program Date" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ProgramName"
        tools:text="Program Name" />

    <EditText
        android:id="@+id/maintain_program_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="one" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ProducerName"
        tools:text="Producer Name" />

    <EditText
        android:id="@+id/maintain_producer_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="one" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="PresenterName"
        tools:text="Presenter Name" />

    <EditText
        android:id="@+id/maintain_presenter_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="one" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="StartTime"
        tools:text="StartTime" />

    <EditText
        android:id="@+id/maintain_starttime_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLength="8"
        android:text="12:30:00"
        tools:text="12:30:00" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Duration"
        tools:text="Duration" />

    <EditText
        android:id="@+id/maintain_programslot_duration_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLength="8"
        android:text="00:30:00"
        tools:text="00:30:00" />


</LinearLayout>
